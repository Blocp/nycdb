####################################
# Postgrest Configuration nycdb
####################################

db-uri       = "postgres://{{ nycdb_database_user }}:{{ nycdb_database_password }}@{{ nycdb_database_host }}:5432/{{ nycdb_database_name }}"

db-schema    = "public"

db-anon-role = "{{ anon_db_user }}"

server-port = {{ postgrest_server_port }}

max-rows = {{ postgrest_max_rows }}
